{
	"name": "ds_blob_internal",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_blob_internal",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"par_filename": {
				"type": "string"
			},
			"par_user": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().par_filename, '.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().par_user",
					"type": "Expression"
				},
				"container": "extract"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}
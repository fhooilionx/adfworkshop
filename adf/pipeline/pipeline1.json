								}
							],
							"outputs": [
								{
									"referenceName": "ds_blob_internal",
									"type": "DatasetReference",
									"parameters": {
										"par_filename": {
											"value": "@item().ENDPOINT",
											"type": "Expression"
										},
										"par_user": "thijs"
									}
								}
							]
						}
					]
				}
			}
		],
		"annotations": []
	}
